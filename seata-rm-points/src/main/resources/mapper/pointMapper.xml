<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.nj.seatarmpoints.mapper.pointMapper">


    <update id="update" parameterType="cn.nj.seatarmpoints.entity.points">
        update points
        set points=points+#{points}
        where
            member_id=#{id}
    </update>
</mapper>
